# Test advanced curve calculation functions
import math from './math_module.vx'

print "\n=== Cubic x³ - 2x² + 3x - 5 ==="
for x in [0, 1, 2]
    set x_str string_from_number(x)
    set result_str string_from_number(math.cubic_polynomial(x, -5, 3, -2, 1))
    set partial string_concat("x=", x_str)
    set full_msg string_concat(partial, string_concat(": ", result_str))
    print full_msg
end